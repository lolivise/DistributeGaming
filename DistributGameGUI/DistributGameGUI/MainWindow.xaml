<Window x:Class="DistributGameGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DistributGameGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Frame x:Name="Main" />
        <Button x:Name="btn_login" Content="Login" HorizontalAlignment="Left" Margin="330,275,0,0" VerticalAlignment="Top" Width="75" Click="btn_login_Click"/>
        <TextBox x:Name="userName" HorizontalAlignment="Left" Height="23" Margin="370,143,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passWord" HorizontalAlignment="Left" Height="23" Margin="370,209,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="261,139,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.3,-0.969"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="267,209,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>